{
  "name": "puppeteer-pro",
  "version": "0.0.0-semantic-release",
  "description": "A simple puppeteer wrapper to enable useful plugins with ease",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "postbuild": "npm run copy:injections && npm run copy:files",
    "copy:injections": "copyfiles -u 1 src/plugins/*/injections/* dist",
    "copy:files": "copyfiles package.json README.md LICENSE dist",
    "test": "mocha --timeout 30000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItzRabbs/puppeteer-pro.git"
  },
  "author": "ItzRabbs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItzRabbs/puppeteer-pro/issues"
  },
  "homepage": "https://github.com/ItzRabbs/puppeteer-pro#readme",
  "keywords": [
    "puppeteer",
    "chrome",
    "automation",
    "plugin",
    "anonymize",
    "detection",
    "stealth"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "ghost-cursor": "^1.1.11",
    "user-agents": "^1.0.1044"
  },
  "devDependencies": {
    "@types/node": "^17.0.42",
    "@types/user-agents": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "chai": "^4.3.6",
    "copyfiles": "^2.4.1",
    "dotenv-safe": "^8.2.0",
    "eslint": "^8.17.0",
    "mocha": "^10.0.0",
    "puppeteer": "^14.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3"
  }
}