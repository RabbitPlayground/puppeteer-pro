{
  "name": "puppeteer-pro",
  "version": "0.0.0-semantic-release",
  "description": "A simple puppeteer wrapper to enable useful plugins with ease",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "npm run copy:injections && npm run copy:files",
    "copy:injections": "copyfiles -u 1 src/plugins/*/injections/* dist",
    "copy:files": "copyfiles package.json README.md LICENSE dist",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fast-facts/puppeteer-pro.git"
  },
  "author": "fast-facts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fast-facts/puppeteer-pro/issues"
  },
  "homepage": "https://github.com/fast-facts/puppeteer-pro#readme",
  "keywords": [
    "puppeteer",
    "chrome",
    "automation",
    "plugin",
    "anonymize",
    "detection",
    "stealth"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "ghost-cursor": "^1.3.0",
    "user-agents": "^1.1.46"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.13",
    "@types/user-agents": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "copyfiles": "^2.4.1",
    "dotenv-safe": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "jest": "^29.7.0",
    "puppeteer": "^22.10.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}